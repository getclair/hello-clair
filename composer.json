{
  "name": "getclair/hello-clair",
  "description": "The Laravel Zero Framework.",
  "keywords": [
    "framework",
    "laravel",
    "laravel zero",
    "console",
    "cli"
  ],
  "type": "project",
  "license": "MIT",
  "support": {
    "source": "https://github.com/getclair/hello-clair"
  },
  "authors": [
    {
      "name": "Adam Campbell",
      "email": "adam@getclair.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "czproject/git-php": "^4.0",
    "hotmeteor/eco-env": "^1.1",
    "laminas/laminas-text": "^2.8",
    "laravel-zero/phar-updater": "^1.0.6",
    "nunomaduro/laravel-console-menu": "^3.2",
    "nunomaduro/laravel-console-task": "^1.6",
    "titasgailius/terminal": "^1.0"
  },
  "require-dev": {
    "laravel-zero/framework": "^8.8",
    "mockery/mockery": "^1.4.3",
    "pestphp/pest": "^1.3"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "builds/clair"
  ]
}
